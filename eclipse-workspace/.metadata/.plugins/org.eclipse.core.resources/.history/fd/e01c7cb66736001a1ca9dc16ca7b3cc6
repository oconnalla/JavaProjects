
public class fibonacci {
	public static void main(String[] args) { 
	    fibonacci(12);
	  }  
	    public static void fibonacci(int count){
	    int n1=0;
	    int n2=1;
	    int n3;
	    //prints the first 2 instances of variables n1 and n2    
	    System.out.print(n1+" "+n2);
	        
	    //i skips the first 2 instances as they are already declared
	    for(int i=2;i<= count;++i){    
	      n3 = n1+n2;    
	      System.out.print(" "+n3);   
	      n1 = n2;    
	      n2 = n3;    
	    }// end of for loop         
	  }// end of fibonacci method

	} //end of class
